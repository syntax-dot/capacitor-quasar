{
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": false,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "@capacitor/core"
    ],
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": false,
    "downlevelIteration": true,
    "noImplicitAny": true
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules",
    "src-capacitor",
    "src-cordova",
    "quasar.config.*.temporary.compiled*"
  ]
}
